{
    "manifest_version": 3,
    "name": "LyricsFinder",
    "version": "1.2",
    "description": "Get the lyrics of any song by providing the song's name and the singer's name. Save favorite lyrics and more.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon.png",
            "48": "icons/icon.png",
            "128": "icons/icon.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    }
}